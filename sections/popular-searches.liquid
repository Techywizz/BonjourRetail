      "max": 32,
      "step": 2,
      "unit": "px",
      "label": "Category Heading Size",
      "default": 18
    },
    {
      "type": "select",
      "id": "category_heading_weight",
      "label": "Category Heading Weight",
      "options": [
        {
          "value": "400",
          "label": "Normal"
        },
        {
          "value": "600",
          "label": "Semi Bold"
        },
        {
          "value": "700",
          "label": "Bold"
        }
      ],
      "default": "600"
    },
    {
      "type": "range",
      "id": "link_size",
      "min": 12,
      "max": 20,
      "step": 1,
      "unit": "px",
      "label": "Link Size",
      "default": 14
    },
    {
      "type": "select",
      "id": "link_weight",
