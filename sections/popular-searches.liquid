{{ 'section-popular-searches.css' | asset_url | stylesheet_tag }}

{%- style -%}
  .popular-searches {
    margin-top: {{ section.settings.margin_top | times: 0.75 | round: 0 }}px;
  }

  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .popular-searches {
      margin-top: {{ section.settings.margin_top }}px;
    }

    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}

<section class="popular-searches color-{{ section.settings.color_scheme }} gradient section-{{ section.id }}-padding{% if section.settings.full_width %} popular-searches--full-width{% endif %}">
  <div class="{% if section.settings.full_width %}page-width-full{% else %}page-width{% endif %}">
    <div class="popular-searches__container">
      {%- if section.settings.main_heading != blank -%}
        <h2 class="popular-searches__heading {{ section.settings.main_heading_alignment }}" 
            style="font-size: {{ section.settings.main_heading_size }}px; font-weight: {{ section.settings.main_heading_weight }};">
          {{ section.settings.main_heading }}
        </h2>
      {%- endif -%}
      
      <div class="popular-searches__content">
        <!-- Men's Links -->
        {%- assign men_has_links = false -%}
        {%- for i in (1..15) -%}
          {%- assign link_key_text = 'men_link_' | append: i | append: '_text' -%}
          {%- assign link_key_url = 'men_link_' | append: i | append: '_url' -%}
          {%- assign link_text = section.settings[link_key_text] -%}
          {%- assign link_url = section.settings[link_key_url] -%}
          {%- if link_text != blank and link_url != blank -%}
            {%- assign men_has_links = true -%}
            {%- break -%}
          {%- endif -%}
        {%- endfor -%}
        
        {%- if section.settings.men_category_heading != blank and men_has_links -%}
        <div class="popular-searches__category">
          <h3 class="popular-searches__category-heading" 
              style="font-size: {{ section.settings.category_heading_size }}px; font-weight: {{ section.settings.category_heading_weight }};">
            {{ section.settings.men_category_heading | default: 'Men' }}
          </h3>
          <div class="popular-searches__links-container">
            {%- for i in (1..15) -%}
              {%- assign link_key_text = 'men_link_' | append: i | append: '_text' -%}
              {%- assign link_key_url = 'men_link_' | append: i | append: '_url' -%}
              {%- assign link_text = section.settings[link_key_text] -%}
              {%- assign link_url = section.settings[link_key_url] -%}
              {%- if link_text != blank and link_url != blank -%}
                <a href="{{ link_url }}" 
                   class="popular-searches__link link link--text"
                   style="font-size: {{ section.settings.link_size }}px; font-weight: {{ section.settings.link_weight }}; color: {{ section.settings.link_color }};">
                  {{ link_text }}
                </a>
                {%- unless forloop.last -%}
                  <span class="popular-searches__separator">|</span>
                {%- endunless -%}
              {%- endif -%}
            {%- endfor -%}
          </div>
        </div>
        {%- endif -%}

